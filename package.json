{
  "name": "goodtables.io",
  "version": "0.1.0",
  "description": "Continuous data validation, as a service.",
  "license": "AGPL-3.0",
  "scripts": {
    "build:dev": "webpack --output-pathinfo --progress --hide-modules",
    "build:prod": "NODE_ENV=production webpack --progress --hide-modules",
    "celery:dev": "celery -A goodtablesio.celery_app worker --loglevel=debug",
    "celery:prod": "celery -A goodtablesio.celery_app worker --loglevel=info",
    "deps": "npm run deps:back && npm run deps:front",
    "deps:back": "pip-compile > requirements.txt",
    "deps:front": "npm shrinkwrap",
    "init": "npm run init:back && npm run init:front",
    "init:back": "pip install --upgrade -r requirements.dev",
    "init:front": "npm install",
    "lint": "npm run lint:back && npm run lint:front",
    "lint:back": "pylama goodtablesio",
    "lint:front": "eslint --ext js,vue frontend",
    "migrate": "alembic upgrade head",
    "prepush": "npm run test && npm run spec",
    "pretest": "npm run lint",
    "server:dev": "FLASK_APP=goodtablesio/app.py FLASK_DEBUG=1 flask run",
    "server:prod": "make migrate && gunicorn --access-logfile - --log-file - goodtablesio.app:app",
    "spec": "NODE_ENV=testing node runspec.js",
    "test": "npm run test:back && npm run test:front",
    "test:back": "py.test --cov goodtablesio --cov-report term-missing",
    "test:front": "NODE_ENV=testing karma start"
  },
  "dependencies": {
    "vue": "^2.1.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.2",
    "babel-core": "^6.0.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.0.0",
    "babel-plugin-istanbul": "^3.1.2",
    "babel-plugin-transform-async-to-generator": "^6.16.0",
    "babel-plugin-transform-class-properties": "^6.22.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.22.0",
    "babel-plugin-transform-es2017-object-entries": "0.0.3",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-polyfill": "^6.22.0",
    "babel-preset-es2015": "^6.0.0",
    "chai": "^3.5.0",
    "chromedriver": "^2.27.2",
    "css-loader": "^0.26.1",
    "eslint": "^3.14.1",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-html": "^2.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "^6.8.0",
    "file-loader": "^0.9.0",
    "http-server": "^0.9.0",
    "husky": "^0.13.1",
    "json-loader": "^0.5.4",
    "karma": "^1.4.1",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sinon-chai": "^1.2.4",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "^2.0.2",
    "mocha": "^3.2.0",
    "nightwatch": "^0.9.12",
    "phantomjs": "^2.1.7",
    "saucelabs": "^1.3.0",
    "selenium-server": "3.0.1",
    "sinon": "^1.17.7",
    "sinon-chai": "^2.8.0",
    "url-loader": "^0.5.7",
    "vue": "^2.1.10",
    "vue-loader": "^10.0.0",
    "vue-style-loader": "^2.0.0",
    "vue-template-compiler": "^2.1.0",
    "webpack": "^2.2.0",
    "webpack-dev-server": "^2.2.0",
    "webpack-merge": "^2.6.1"
  }
}
